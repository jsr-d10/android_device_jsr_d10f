diff --git a/lzop/B/dos16/bc.bat b/lzop/B/dos16/bc.bat
new file mode 100644
index 0000000..20c00a6
--- /dev/null
+++ b/lzop/B/dos16/bc.bat
@@ -0,0 +1,19 @@
+@echo // Copyright (C) 1996-2010 Markus F.X.J. Oberhumer
+@echo //
+@echo //   DOS 16-bit
+@echo //   Borland C/C++
+@echo //
+@call b\prepare.bat
+@if "%BECHO%"=="n" echo off
+
+
+bcc -ml -f- -O1 -w -Isrc -Iacc %CFI% -e%BEXE% @b\src.rsp %BLDLIBS%
+@if errorlevel 1 goto error
+
+
+@call b\done.bat
+@goto end
+:error
+@echo ERROR during build!
+:end
+@call b\unset.bat
