+void e_help(void)
+{
+    if (opt_pgm == PGM_LZOP)
+        help();
+    else if (opt_pgm == PGM_UNLZOP)
+        help();
+    else if (opt_pgm == PGM_OCAT)
+    {
+        if (opt_stdin)
+            check_stdin(&fi);
+        if (opt_stdout)
+            check_stdout(&fo);
+        usage();
+    }
+    else
+        help();
+    e_exit(EXIT_USAGE);
