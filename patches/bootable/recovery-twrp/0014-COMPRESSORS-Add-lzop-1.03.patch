+
+void set_cmd(int cmd)
+{
+    if (cmd == CMD_COMPRESS && (opt_pgm == PGM_UNLZOP || opt_pgm == PGM_OCAT))
+        return;
+#if 0
+    if (opt_cmd != CMD_NONE && cmd != opt_cmd)
+    {
+        fprintf(stderr,"%s: multiple commands given\n",argv0);
+        e_usage();
+    }
+    opt_cmd = cmd;
+#else
+    /* gzip: commands have a certain priority */
+    if (cmd > opt_cmd)
+        opt_cmd = cmd;
+#endif
+}
