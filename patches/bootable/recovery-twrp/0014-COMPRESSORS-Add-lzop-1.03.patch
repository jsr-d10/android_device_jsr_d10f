+                for path in $paths; do
+                    if [ $minusx "$path/$prg" ] && [ ! -d "$path/$prg" ]; then
+                        comp_prg="$prg"
+                        break
+                    fi
+                done
+                if [ ".$comp_prg" != . ]; then
+                    break
+                fi
+            done
