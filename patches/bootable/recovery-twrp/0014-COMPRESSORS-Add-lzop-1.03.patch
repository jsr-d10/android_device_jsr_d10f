+/*************************************************************************
+//
+**************************************************************************/
+
+void t_sub1(lzo_bytep p, lzo_uint l)
+{
+    unsigned char b = 0;
+
+    if (l > 0) do {
+        *p -= b;
+        b += *p++;
+    } while (--l > 0);
+}
+
+
+void t_add1(lzo_bytep p, lzo_uint l)
+{
+    unsigned char b = 0;
+
+    if (l > 0) do {
+        b += *p;
+        *p++ = b;
+    } while (--l > 0);
+}
+
+
+/*************************************************************************
+//
+**************************************************************************/
+
+void t_sub(lzo_bytep p, lzo_uint l, int n)
+{
+    unsigned char b[16];
+    int i;
+
+    assert(n > 0 && n <= (int)sizeof(b));
+    if (l <= (lzo_uint)n)
+        return;
+
+    n--;
+    i = n; do b[i] = 0; while (--i >= 0);
+
+    i = n;
+    do {
+        *p -= b[i];
+        b[i] += *p++;
+        if (--i < 0)
+            i = n;
+    } while (--l > 0);
+}
+
+
+void t_add(lzo_bytep p, lzo_uint l, int n)
+{
+    unsigned char b[16];
+    int i;
+
+    assert(n > 0 && n <= (int)sizeof(b));
+    if (l <= (lzo_uint)n)
+        return;
+
+    n--;
+    i = n; do b[i] = 0; while (--i >= 0);
+
+    i = n;
+    do {
+        b[i] += *p;
+        *p++ = b[i];
+        if (--i < 0)
+            i = n;
+    } while (--l > 0);
+}
+
+
+/*************************************************************************
+//
+**************************************************************************/
+
+void t_mtf(lzo_bytep p, lzo_uint l)
+{
+    unsigned char b[256];
+    unsigned char c;
+    unsigned i;
+
+    if (l <= 1)
+        return;
+
+    i = 256; do { --i; b[i] = (unsigned char) i; } while (i != 0);
+
+    do {
+        c = *p;
+        for (i = 0; c != b[i]; )
+            i++;
+        *p++ = (unsigned char) i;
+        if (i > 0)
+        {
+            do b[i] = b[i-1]; while (--i > 0);
+            b[0] = c;
+        }
+    } while (--l > 0);
+}
+
+
+void t_unmtf(lzo_bytep p, lzo_uint l)
+{
+    unsigned char b[256];
+    unsigned char c;
+    unsigned i;
+
+    if (l <= 1)
+        return;
+
+    i = 256; do { --i; b[i] = (unsigned char) i; } while (i != 0);
+
+    do {
+        i = *p;
+        c = b[i];
+        *p++ = c;
+        if (i > 0)
+        {
+            do b[i] = b[i-1]; while (--i > 0);
+            b[0] = c;
+        }
+    } while (--l > 0);
+}
