+            paths="`echo $PATH |\
+                    sed -e 's%/*:%:%g' -e 's%/*$%%' \
+                        -e 's/^:/.:/' -e 's/::/:.:/g' -e 's/:$/:./' \
+                        -e 's/:/ /g'`"
