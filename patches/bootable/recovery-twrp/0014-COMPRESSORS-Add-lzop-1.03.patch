+
+#ifndef __SCREEN_H
+#define __SCREEN_H 1
+
+#if defined(USE_SCREEN)
+
+
+/*************************************************************************
+//
+**************************************************************************/
+
+struct screen_data_t;
+struct screen_t;
+typedef struct screen_t screen_t;
+
+struct screen_t
+{
+/* public: */
+    void (*destroy)(screen_t *s);
+    void (*finalize)(screen_t *s);
+
