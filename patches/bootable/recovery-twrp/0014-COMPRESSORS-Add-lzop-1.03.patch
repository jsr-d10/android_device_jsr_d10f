+    fprintf(f," ");
+    print_version(f,h->version_needed_to_extract);
+    fprintf(f,"  Fl: 0x%08lx", (long) h->flags);
+    fprintf(f,"  Mo: 0%011lo", (long) h->mode);
+    fprintf(f,"  Me: %d/%d", h->method, h->level);
+    fprintf(f,"  OS: ");
+    print_os(f,h->flags);
+    if (h->filter)
+        fprintf(f,"  Fi: %3ld", (long) h->filter);
+    fprintf(f,"\n");
