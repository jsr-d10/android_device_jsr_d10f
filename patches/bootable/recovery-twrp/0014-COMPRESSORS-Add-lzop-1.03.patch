+                    if [ ".$opt_t" = .yes ]; then
+                        echo "chgrp $opt_g ${adir}/${file}.${m}${comp_suf}"
+                    fi
+                    chgrp $opt_g ${adir}/${file}.${m}${comp_suf} || shtool_exit $?
+                fi
